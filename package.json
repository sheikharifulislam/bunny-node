{
    "name": "@bunny.net/node",
    "version": "0.0.1",
    "description": "",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node src/index.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist",
        "postinstall": "npm run build"
    },
    "keywords": [],
    "author": "Sheikh Ariful Islam",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/sheikharifulislam/bunny-node.git"
    },
    "bugs": {
        "url": "https://github.com/sheikharifulislam/bunny-node/issues"
    },
    "homepage": "https://github.com/sheikharifulislam/bunny-node#readme",
    "packageManager": "pnpm@10.6.2",
    "devDependencies": {
        "@types/node": "^24.3.0",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "typescript": "^5.9.2"
    }
}
